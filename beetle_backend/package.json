{
  "name": "beetle-backend",
  "version": "1.0.0",
  "description": "Backend for Beetle - Git-based collaboration with Branch-Level Intelligence",
  "main": "src/index.cjs",
  "scripts": {
    "start": "node src/index.cjs",
    "dev": "nodemon src/index.cjs",
    "dev:stable": "node src/index.cjs",
    "setup": "node setup-env.js",
    "setup:quick": "./create-env.sh",
    "test": "jest",
    "test:ai": "node test-ai-pipeline.js",
    "test:security": "node test-security.js",
    "test:security:comprehensive": "node test-security-comprehensive.js",
    "generate:security": "node generate-security-config.js",
    "validate:security": "node -e \"require('./src/utils/config-validator.cjs').validateOnStartup()\""
  },
  "dependencies": {
    "@octokit/rest": "^20.0.2",
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "lowdb": "^6.1.1",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "node-cache": "^5.1.2",
    "playwright": "^1.40.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "github",
    "collaboration",
    "git",
    "branch-intelligence",
    "open-source"
  ],
